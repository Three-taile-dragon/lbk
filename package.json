    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.28",
    "prettier": "^2.7.1",
    "qs": "^6.11.0",
