    "pinia-plugin-persist": "^1.0.0",
    "pinia-plugin-persistedstate": "^3.0.2",
    "vue": "^3.2.45",
    "vue-request": "next",
    "vue-router": "next"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
